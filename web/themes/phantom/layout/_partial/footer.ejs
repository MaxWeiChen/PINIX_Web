<!-- Footer -->
<footer id="footer">
    <div class="inner">
        <!--<section>
            <h2>About</h2>
            <div>
                <%- theme.about_footer %>
            </div>
        </section>
        -->
        <section>
            <h2>Follow</h2>
            <ul class="icons">
                <% if (theme.twitter_url) { %>
                    <li><a href="<%- theme.twitter_url %>" class="icon style2 fa-twitter" target="_blank"><span class="label">Twitter</span></a></li>
                <% } %>
                <% if (theme.facebook_url) { %>
                    <li><a href="<%- theme.facebook_url %>" class="icon style2 fa-facebook-f" target="_blank"><span class="label">Facebook</span></a></li>
                <% } %>
                <% if (theme.instagram_url) { %>
                    <li><a href="<%- theme.instagram_url %>" class="icon style2 fa-instagram" target="_blank"><span class="label">Instagram</span></a></li>
                <% } %>
                <% if (theme.github_url) { %>
                    <li><a href="<%- theme.github_url %>" class="icon style2 fa-github" target="_blank"><span class="label">GitHub</span></a></li>
                <% } %>
                <% if (theme.patreon_url) { %>
                    <li><a href="<%- theme.patreon_url %>" class="icon style2 fab fa-patreon" target="_blank"><span class="label">Patreon</span></a></li>
                <% } %>
                <% if (theme.email_url) { %>
                    <li><a href="<%- theme.email_url %>" class="icon style2 fa-envelope-o"><span class="label">Email</span></a></li>
                <% } %>
                <% if (theme.rss_url) { %>
                    <li><a href="<%- theme.rss_url %>" class="icon style2 fa-rss" target="_blank"><span class="label">RSS</span></a></li>
                <% } %>
            </ul>
        </section>
        <ul class="copyright">
            <li>&copy; Untitled. All rights reserved</li>
            <li>Design: <a href="http://html5up.net" target="_blank">HTML5 UP</a></li>
            <li>Hexo: <a href="http://www.codeblocq.com/" target="_blank">Jonathan Klughertz</a></li>
            <li>PINIX games studio</li>
        </ul>
    </div>
</footer>