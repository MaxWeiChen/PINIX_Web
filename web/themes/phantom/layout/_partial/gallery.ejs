<p>
<table>
	<td>
		<h1 class="photo"><%- item.photoTitle || "Images" %></h1>
		<% if (item.photoPackage){ %>
    </td>
    <td>
		<a href="<%- config.root %><%- item.photoPackage %>" class="button small" target=_self>
			Download All
		</a>
		<% } %>
	</td>
</table>

<div class="gallery-container">
    <% if (item.photos.length){ %>
        <% item.photos.forEach(function(photo){ %>
            <div class="picture-container">
                <a href="#" data-featherlight="<%- config.root %><%- photo %>">
                    <img src="<%- config.root %><%- photo %>" class="img-thumbnail">
                </a>
            </div>
        <% }); %>
    <% } %>
</div>